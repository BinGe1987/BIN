<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <!-- 线的宽度，颜色灰色 -->
            <stroke android:width="0dp"
                android:color="@color/color_tab_u"></stroke>
            <solid android:color="@color/color_tab_u"/>
   			<corners android:radius="300dp" />
        </shape>
    </item>
    
    <item >
        <shape android:shape="rectangle">
            <!-- 线的宽度，颜色灰色 -->
   	 		<stroke
        		android:width="0dp"
        		android:color="@color/color_tab_d" >
    		</stroke>
            <solid android:color="@color/color_tab_d"/>
   			<corners android:radius="300dp" />
        </shape>
    </item>

</selector>