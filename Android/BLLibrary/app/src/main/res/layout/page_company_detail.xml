<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffffff"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical" >

    <include layout="@layout/title" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical" >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:padding="5dp"
                android:text="企业名称"
                android:textColor="@color/contact_text_detail_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/company_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:background="@null"
                android:text="135-1009-0191"
                android:textSize="14sp" />

            <include layout="@layout/line" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:padding="5dp"
                android:text="客户类型"
                android:textColor="@color/contact_text_detail_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/company_type"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:background="@null"
                android:text="135-1009-0191"
                android:textSize="14sp" />

            <include layout="@layout/line" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:padding="5dp"
                android:text="公司地址"
                android:textColor="@color/contact_text_detail_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/company_addr"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:background="@null"
                android:text="135-1009-0191"
                android:textSize="14sp" />

            <include layout="@layout/line" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:padding="5dp"
                android:text="公司电话"
                android:textColor="@color/contact_text_detail_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/company_tel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:background="@null"
                android:text="135-1009-0191"
                android:textSize="14sp" />

            <include layout="@layout/line" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:padding="5dp"
                android:text="传真"
                android:textColor="@color/contact_text_detail_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/fax"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:background="@null"
                android:text="135-1009-0191"
                android:textSize="14sp" />

            <include layout="@layout/line" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:padding="5dp"
                android:text="邮箱"
                android:textColor="@color/contact_text_detail_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/company_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:background="@null"
                android:text="135-1009-0191"
                android:textSize="14sp" />

            <include layout="@layout/line" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:padding="5dp"
                android:text="纳税人识别号"
                android:textColor="@color/contact_text_detail_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/finance_identify_id"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:background="@null"
                android:text="135-1009-0191"
                android:textSize="14sp" />

            <include layout="@layout/line" />

           

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:padding="5dp"
                android:text="开户银行"
                android:textColor="@color/contact_text_detail_color"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/open_bank"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="-4dp"
                android:background="@null"
                android:text="135-1009-0191"
                android:textSize="14sp" />

            <include layout="@layout/line" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>